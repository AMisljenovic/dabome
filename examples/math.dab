fn faktorijel(n) {
  let result = 1
  let i = 1
  while (i < n + 1) {
    result = result * i
    i = i + 1
  }
  result
}

print(faktorijel(1))
print(faktorijel(5))
print(faktorijel(10))

fn stepen(baza, exp) {
  let result = 1
  let i = 0
  while (i < exp) {
    result = result * baza
    i = i + 1
  }
  result
}

print(stepen(2, 8))
print(stepen(3, 4))
print(stepen(10, 3))
