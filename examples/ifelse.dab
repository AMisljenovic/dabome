fn max(a, b) {
  if (a > b) {
    a
  } else {
    b
  }
}

print("max(10, 20) = " + max(10, 20))
print("max(50, 30) = " + max(50, 30))

fn apsolutna(n) {
  if (n < 0) {
    0 - n
  } else {
    n
  }
}

print("abs(-5) = " + apsolutna(0 - 5))
print("abs(10) = " + apsolutna(10))

