fn fizzbuzz(n) {
  let i = 1
  while (i < n + 1) {
    if (i % 15 == 0) {
      print("FizzBuzz")
    } else if (i % 3 == 0) {
      print("Fizz")
    } else if (i % 5 == 0) {
      print("Buzz")
    } else {
      print(i)
    }
    i = i + 1
  }
}

print("FizzBuzz 1-15:")
fizzbuzz(15)
